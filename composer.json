{
    "name": "hi2binay/laravel-package-generator",
    "description": "This is package is use to create laravel package skeleton",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel",
        "package",
        "generator"
    ],
    "authors": [
        {
            "name": "Binay Kumar",
            "email": "binay.cs@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1.1",
        "laravel/framework": ">=10.10"
    },
    "autoload": {
        "psr-4": {
            "BKP\\LaravelPackageGenerator\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BKP\\LaravelPackageGenerator\\LaravelPackageGeneratorServiceProvider"
            ],
            "aliases": {

            }
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
